//finding factorial of number 192211677
#include <stdio.h>

int main() {
	printf("name : B.sandeep\n");
    int n;
    printf("Enter a positive integer: ");
    scanf("%d", &n);

    int factorial = 1;

    if (n < 0) {
        printf("Factorial is not defined for negative numbers.\n");
    } else {
        for (int i = 1; i <= n; i++) {
            factorial *= i;
        }
        printf("Factorial of %d is %d\n", n, factorial);
    }

    return 0;
}
